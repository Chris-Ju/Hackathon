doctype html
html
  head(lang="en")
    meta(charset="UTF-8")
    |     
    title
    |     
    link(type="text/css", href="stylesheets/styles.css", rel="stylesheet")
    |     
    script(src="javascripts/jquery-1.11.2.min.js", type="text/javascript")
    |     
    script(src="javascripts/modernizr-2.6.2.min.js", type="text/javascript")
    |     
    script(src="javascripts/starbg.js", type="text/javascript")
    |
    script(src="javascripts/regist.js", type="text/javascript")     
    |     
    style.
      *{ margin:0; padding:0;}
      canvas{ display: block; margin: 0; width:100%; height:100%; position: fixed; left: 0; right: 0; top:0; bottom:0;
      z-index: -1; }
      .content{ width: 1144px; margin: 0 auto; padding-top:200px;}
      .rabitBg{ position:fixed; left:0; top:50%;}
  body  
    canvas#fullstarbg 你的浏览器不支持canvas标签
    |    
    .content
      img.rabitBg(src="images/rabit.png")
      |
      #large-header.large-header
      canvas#demo-canvas
      |                 
      .logo_box
        .input_outer
          span.u_user
          |
          input#userName.text(name='username', type='text', placeholder='请输入用户名', autocomplete='off')
        |
        .input_outer
          .pass-box
            span.u_password
            |
            input#password.text(name='password', type='password', placeholder='请输入密码', autocomplete='off')
        |
        .input_outer
          .pass-box
            span.r_password
            |
            input#Rpassword.text(name='Rpassword', type='password', placeholder='请再次输入密码', autocomplete='off')
        |
        .input_outer
          span.u_phone
          |
          input#phone.text(name='phone', type='text', placeholder='请输入电话', autocomplete='off')
        |
        .input_outer
          span.u_email
          |
          input#email.text(name='email', type='text', placeholder='请输入邮箱', autocomplete='off')
        |
        #warning.input_outer
          span.u_warning
          |
          input.text(name='warning', type='text', value=value, readonly='')
        button#reset.submit 清空
        button#submit.submit 注册
