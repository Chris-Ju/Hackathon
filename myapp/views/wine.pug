    header.cd-morph-dropdown
      a.nav-trigger(href="#0") Open Nav
        span(aria-hidden="true")
      nav.main-nav
        ul
          li(class="has-dropdown home" data-content="home")
            a.myhome(href="#0") 首页
          li(class="has-dropdown gallery" data-content="about")
            a.mywine(href="#0") 我的故事
          li(class="has-dropdown links" data-content="pricing")
            a.mycandy(href="#0") 我的糖果
          li(class="has-dropdown button" data-content="contact")
            a.exit(href="#0") 退出登陆
      .morph-dropdown-wrapper
        .dropdown-list
          ul
            li#home(class="dropdown home")
              a.myhome.label(href="#0") 首页
            li#about(class="dropdown gallery")
              a.mywine.label(href="#0") 我的故事
            li#pricing(class="dropdown links")
              a.mycandy.label(href="#0") 我的糖果
            li#contact(class="dropdown button")
              a.exit.label(href="#0") 退出登陆
